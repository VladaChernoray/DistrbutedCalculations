    <servlet-mapping>
        <servlet-name>ProductCRUDServlet</servlet-name>
        <url-pattern>/ProductInsert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductCRUDServlet</servlet-name>
        <url-pattern>/ProductUpdate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
